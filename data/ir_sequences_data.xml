<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="seq_carrier_id" model="ir.sequence"> 
            <field name="name">Autoincrement carrier ID</field> 
            <field name="code">delivery.carrier_id</field> 
            <field name="prefix">CAR</field> 
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_customer_id" model="ir.sequence"> 
            <field name="name">Autoincrement customer ID</field> 
            <field name="code">delivery.customer_id</field> 
            <field name="prefix">CUS</field> 
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        
		<record id="seq_order_id" model="ir.sequence"> 
            <field name="name">Autoincrement order ID</field> 
            <field name="code">delivery.order_id</field> 
            <field name="prefix">ORD</field> 
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>